# Premium Subscription System Environment Variables
# Copy to .env and replace with your actual credentials

# Instamojo Configuration
INSTAMOJO_API_KEY=your_instamojo_api_key_here
INSTAMOJO_AUTH_TOKEN=your_instamojo_auth_token_here

# Dipay Configuration  
DIPAY_API_KEY=your_dipay_api_key_here
DIPAY_SECRET=your_dipay_secret_here
REACT_APP_DIPAY_KEY_ID=your_dipay_key_id_here

# ZeptoMail Configuration (for transactional emails)
ZEPTO_API_KEY=your_zeptomail_api_key_here

# Supabase Configuration (should already exist)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Admin Emails (comma-separated)
ADMIN_EMAILS=admin@testingvala.com,owner@testingvala.com